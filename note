mkdir -(make directory)
mkdir one two three 
mkdir -p /one/two/three
mkdir -m 700 perfile

ls  (List )
ls -l (long list format)
ls -a (hide file)
ls -R (show all folder and file with this one command )
ls -h (human read able show MB ,GB ,KB from this folder and file )
ls -F ( / - folder @ - symbolic * -execte file )
